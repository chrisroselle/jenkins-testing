@Library('cr-common') _

pipeline {
    agent any
    
    parameters {
        choice(name: 'example2', description: p.example2.description, choices: p.example2.choices)
    }
    
    stages {
        stage('CR') {
            steps {
                script {
                    def interesting = p.example2.choices[0]
                    if (example2 == interesting) {
                        echo "this is interesting"
                    }
                }
            }
        }
    }
    
    options {
        buildDiscarder(
            logRotator(
                //numToKeepStr: '30',
                daysToKeepStr: '30'
            )
        )
    }
}